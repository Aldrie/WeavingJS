{
  "extends": ["airbnb-typescript"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "no-console": 0,
    "max-classes-per-file": 0,
    "import/prefer-default-export": 0,
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "no-plusplus": 0,
    "no-param-reassign": 0,
    "@typescript-eslint/no-use-before-define": 0
  }
}