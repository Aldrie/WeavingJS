{
  "name": "weaving-js",
  "version": "1.0.0",
  "main": "build/index.js",
  "license": "MIT",
  "author": "AldriÃª Mizobuti <aldriemizobuti@outlook.com>",
  "scripts": {
    "start": "node build",
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules --no-notify src",
    "build": "npm-run-all \"build:ts\" \"build:package\"",
    "build:ts": "babel src --extensions \".js,.ts\" --out-dir build --copy-files --no-copy-ignored",
    "build:package": "pkg package.json --output \"packages/WeavingJs\"",
    "lint": "eslint src --ext .js,.ts"
  },
  "bin": "build/index.js",
  "pkg": {
    "assets": [
      "build/templates/*.ejs"
    ],
    "output": "packages/WeavingJs",
    "targets": [
      "node12-win-x64"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/ejs": "^3.0.5",
    "@types/node": "^14.14.21",
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "4.0.8",
    "npm-run-all": "^4.1.5",
    "pkg": "^4.4.9",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "canvas": "^2.6.1",
    "colors": "^1.4.0",
    "ejs": "^3.1.5",
    "image-size": "^0.9.3",
    "open-file-explorer": "^1.0.2"
  }
}
